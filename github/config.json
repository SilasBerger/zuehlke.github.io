{
  "remote_name": "origin",
  "workdir_root_dirname": "_zuehlke.github.io_workdir",
  "target_branch": "playground/automation-dev",
  "no_commit": true,
  "push_always": true,
  "github_api_token_envvar": "GITHUB_API_TOKEN",
  "rate_limit_buffer_sec": 60,
  "request_delay_sec": 2,
  "rate_limit_max_age_sec": 60,
  "max_retries": 2,
  "data_dir_path": ["src", "data"],
  "contributions_filename": "contributions.json",
  "people_filename": "people.json"
}
